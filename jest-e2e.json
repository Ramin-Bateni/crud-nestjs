{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "moduleFileExtensions": ["ts", "js", "json"],
  "testMatch": ["**/?(*.)+(e2e-spec).ts"],
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1",
    "^@/(.*)$": "<rootDir>/src/$1"
  },
  "collectCoverageFrom": ["src/**/*.ts"],
  "coverageDirectory": "coverage"
}
